@model IEnumerable<MeetingVL.Models.Evaluate>

@{
    ViewBag.Title = "Index";
    Layout = null;
    int action = 0;
    if (TempData["action"] != null)
    {
        action = int.Parse(TempData["action"].ToString());
    }
}
@if (action == 1)
{
    if (Model.Count() != 0)
    {
        foreach (var item in Model)
        {
            <td>@item.Point</td>
        }
    }
    else
    {

    }
}
else
{
    if (Model.Count() != 0)
    {
        foreach (var item in Model)
        {
            <a class="Info p-0" href="@Url.Action("Details", "Evaluates", new { id = item.ID })">
                <i class="fa ft-clipboard font-medium-3 mr-2"></i>
            </a>
        }
    }
    else
    {

    }
}



